<html lang='et' class='first-page'>

  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />

    <link
      rel='stylesheet'
      href='https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200'
    />

    <!-- Static fails in /public folder.  -->

    <link rel='stylesheet' href='/css/main.css' />
    <title>Haapsalu kolledži ainekursused</title>
  </head>
  <body>
    <a
      class='btn skip absolute left-[-1000px] mt-4 ml-4 px-4 py-2 bg-white'
      href='#main-content'
    >Liigu põhisisu juurde</a>
    {{{body}}}
    <footer class="relative z-10">
      {{> footer }}
    </footer>
    <script>
      window.addEventListener('scroll', function () { var scrollPosition =
      window.pageYOffset; // Adjust the parallax background scroll speed var
      parallaxBg = document.querySelector('.parallax-bg-slower');
      parallaxBg.style.transform = 'translateY(' + -scrollPosition * 0.5 +
      'px)'; // Adjust the slower parallax background scroll speed var
      parallaxBgSlower = document.querySelector('.parallax-bg-slow');
      parallaxBgSlower.style.transform = 'translateY(' + -scrollPosition * 0.8 +
      'px)'; // Adjust the main section scroll speed var mainSection =
      document.querySelector('.first-page-content'); mainSection.style.transform
      = 'translateY(' + -scrollPosition * 0.9 + 'px)'; });
    </script>
  </body>
</html>